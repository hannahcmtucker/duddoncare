{
  "name": "duddoncare",
  "version": "1.0.0",
  "private": true,
  "description": "duddoncare",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
